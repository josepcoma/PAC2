#    Annotation properties
#################################################################

###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#sameAs
:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#hasOriginDenomination
:hasOriginDenomination rdf:type owl:ObjectProperty ;
                       rdfs:domain :Winery ;
                       rdfs:range :OriginDenomination .


###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#hasPrice
:hasPrice rdf:type owl:ObjectProperty ;
          rdfs:domain :Price ;
          rdfs:range [ rdf:type owl:Restriction ;
                       owl:onProperty :hasPrice ;
                       owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                       owl:onClass :Price
                     ] .


###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#hasType
:hasType rdf:type owl:ObjectProperty ,
                  owl:AsymmetricProperty ;
         rdfs:domain :Wine ;
         rdfs:range :Type .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#hasRating
:hasRating rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Rating ;
           rdfs:range xsd:decimal .


###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#hasYear
:hasYear rdf:type owl:DatatypeProperty ;
         rdfs:domain :Year ;
         rdfs:range xsd:integer .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#Acidity
:Acidity rdf:type owl:Class ;
         rdfs:subClassOf :Type ;
         rdfs:comment "Acidity score, defined as wine's “pucker” or tartness; it's what makes a wine refreshing and your tongue salivate and want another sip [from 1-5]" .


###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#Age
:Age rdf:type owl:Class ;
     rdfs:subClassOf :Wine ;
     rdfs:comment "Characteristics of the wine according to its age" .


###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#Aging
:Aging rdf:type owl:Class ;
       rdfs:subClassOf :Age ;
       rdfs:comment "Aging wine" .


###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#AlcoholLevel
:AlcoholLevel rdf:type owl:Class ;
              rdfs:subClassOf :Type ;
              rdfs:comment "Alcohol level of wine, measured in degrees" .


###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#AltitudeOfYardGrapes
:AltitudeOfYardGrapes rdf:type owl:Class ;
                      rdfs:subClassOf :Winery ;
                      rdfs:comment "Altitude of the vineyard where the grapes were grown" .


###  http://www.semanticweb.org/coma/ontologies/2023/3/Wine_PAC2#Body
:Body rdf:type owl:Class ;
      rdfs:subClassOf :Type ;
      rdfs:comment "Body score, defined as the richness and weight of the wine in your mouth [from 1-5]" .


